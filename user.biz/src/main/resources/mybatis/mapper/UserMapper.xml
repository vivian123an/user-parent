  <sql id="userQuery_whereConditions">
	 <if test="query.userName != null">
	  	and us.user_name like #{query.userName}
	 </if>
	 <if test="query.email != null">
	  	and us.email like #{query.email}
	 </if>
	 <if test="query.mobile != null">
	  	and us.mobile like #{query.mobile}
	 </if>
	 <if test="query.groupId != null">
	  	and us.group_id = #{query.groupId}
	 </if>
	 <if test="query.pName != null">
	  	and us2.user_name = #{query.pName}
	 </if>
	 <if test="query.inviteName != null">
	  	and us3.user_name = #{query.inviteName}
	 </if>
  </sql>
  
  <select id="get" resultMap="ResultMapWithBLOBs" parameterType="java.lang.Integer" >
