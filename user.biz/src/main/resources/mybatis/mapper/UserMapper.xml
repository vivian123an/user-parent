  <select id="getUserListPage" parameterType="map" resultMap="UserResultMap" useCache="false">
  	  <include refid="getUserList"/>
  </select>
  <sql id="getUserList">
  	  select distinct 
  	  	<include refid="User_Column_List"/>
	  from ve_user us
	  left join ve_user_group ug on ug.id = us.group_id
	  left join ve_user_level ul on ul.id = us.level_id
	  left join ve_user us2 on us2.id = us.pid
	  left join ve_user us3 on us3.id = us.invite_id
  	  <where>
	  	<include refid="userQuery_whereConditions"/>
  	  </where>
  	  order by us.id desc
  </sql>
  
  
  <select id="getSimpleUserListPage" parameterType="map" resultMap="BaseResultMap" useCache="false">
